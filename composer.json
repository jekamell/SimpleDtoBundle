{
    "name": "jekamell/simple-dto-bundle",
    "description": "Allow to configure dto objects in easy way, and split persistence and representative application layers",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "dto", "data transfer object", "rest", "restful", "serialization"],
    "type": "symfony-bundle",
    "authors": [
        {"name": "Evgeniy Malykhin", "email": "55jeka@gmail.com"}
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/symfony": ">=3.1",
        "doctrine/orm": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Mell\\Bundle\\SimpleDtoBundle\\": ""
        }
    },
    "suggest": {
        "nelmio/NelmioApiDocBundle": "Generates documentation for your REST API from annotations"
    }
}
